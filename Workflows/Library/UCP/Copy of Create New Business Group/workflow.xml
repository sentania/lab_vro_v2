  <workflow-item name="item13" out-name="item14" type="task">
    <display-name><![CDATA[Get Full Virtual Wire Names]]></display-name>
    <script encoded="false"><![CDATA[var networks = [];
var MkeVwFull;
var FrkVwFull;
allNetworks = cluster.network;

for(n in allNetworks) {
	netName = allNetworks[n].name;
	// Rips vDSwitch tag off of name.
	parLoc = netName.indexOf("(");
	name = netName.slice(0,parLoc-1);
	// Pushes just name of network to array
	//System.log(name);
	networks.push(name);
}

System.log("Found " +networks.length + " networks in the " + cluster.name + " cluster.");
//var vw = virtualWireMke;

//Find the network that has the virtualwire name in it for MKE.
System.log("Looking for a network with the string: "+virtualWireMke);

for(n in networks) {
	if(networks[n].search(virtualWireMke) > -1) {
		System.log("Found: "+networks[n]);
		MkeVwFull = networks[n];
		break;
	}
}

//Find the network that has the virtualwire name in it for FRK.
System.log("Looking for a network with the string: "+virtualWireFrk);

for(n in networks) {
	if(networks[n].search(virtualWireFrk) > -1) {
		System.log("Found: "+networks[n]);
		FrkVwFull = networks[n];
		break;
	}
}]]></script>
    <in-binding>
      <bind name="cluster" type="VC:ClusterComputeResource" export-name="cluster"/>
      <bind name="virtualWireMke" type="string" export-name="virtualWireMke"/>
      <bind name="virtualWireFrk" type="string" export-name="virtualWireFrk"/>
    </in-binding>
    <out-binding>
      <bind name="MkeVwFull" type="string" export-name="MkeVwFull"/>
      <bind name="FrkVwFull" type="string" export-name="FrkVwFull"/>
    </out-binding>
    <position y="128.13636363636363" x="444.5"/>
